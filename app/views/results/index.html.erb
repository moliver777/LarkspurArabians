<h1>Listing results</h1>

<table>
  <tr>
    <th>Horse</th>
    <th>Date</th>
    <th>Competition</th>
    <th>Placing</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @results.each do |result| %>
  <tr>
    <td><%= result.horse_id %></td>
    <td><%= result.date %></td>
    <td><%= result.competition %></td>
    <td><%= result.placing %></td>
    <td><%= result.description %></td>
    <td><%= link_to 'Show', result %></td>
    <td><%= link_to 'Edit', edit_result_path(result) %></td>
    <td><%= link_to 'Destroy', result, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Result', new_result_path %>
